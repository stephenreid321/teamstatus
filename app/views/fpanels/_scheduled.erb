
<div class="well">
  <h3><img src="/images/facebook.png" /> <%=@fconnection.user['name']%> &rsaquo; Scheduled</h3>
</div>

<% if @fstatuses.empty? %>
  <div class="alert"><strong>Well fancy that.</strong> You haven't scheduled anything yet.</div>
<% else %>
  <table class="table">
    <% @fstatuses.each do |fstatus|  %>
      <%= partial :'fpanels/fstatus', :locals => {:post => fstatus} %>
    <% end %>
  </table>
  
  
  
  
  
<% end %>




